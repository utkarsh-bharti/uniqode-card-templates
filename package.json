{
  "name": "@uniqode/card-templates",
  "version": "1.0.0",
  "description": "Universal Web Components library for digital business card templates. Framework-agnostic, customizable, and production-ready.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "npm run build:lib && npm run build:storybook",
    "build:lib": "webpack --mode=production",
    "build:storybook": "storybook build",
    "build:dev": "webpack --mode=development --watch",
    "start": "npm run dev",
    "prepublishOnly": "npm run build:lib",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "test": "npm run test:integration",
    "test:integration": "node test/integration.js",
    "size": "npm run build:lib && ls -lh dist/",
    "clean": "rimraf dist/ storybook-static/",
    "prepack": "npm run clean && npm run build:lib",
    "postpack": "npm run clean"
  },
  "keywords": [
    "web-components",
    "custom-elements",
    "card-templates",
    "digital-business-cards",
    "vcard",
    "templates",
    "uniqode",
    "beaconstac",
    "business-cards",
    "contact-cards",
    "profile-cards",
    "react-components",
    "angular-components",
    "vue-components",
    "framework-agnostic",
    "shadow-dom",
    "responsive-design",
    "customizable",
    "professional"
  ],
  "author": "Uniqode Team <dev@uniqode.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uniqode/card-templates.git"
  },
  "bugs": {
    "url": "https://github.com/uniqode/card-templates/issues"
  },
  "homepage": "https://github.com/uniqode/card-templates#readme",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@playwright/test": "^1.40.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/html-webpack5": "^8.6.14",
    "@types/jest": "^29.5.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^6.11.0",
    "eslint": "^8.50.0",
    "express": "^4.18.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0"
  },
  "peerDependencies": {
    "@webcomponents/webcomponentsjs": "^2.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
